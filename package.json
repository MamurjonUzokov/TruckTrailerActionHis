{
  "name": "automation",
  "version": "1.0.0",
  "description": "Automation API Testing(Integration Testing) with Jenkins",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "api-jma-action-history-release": "newman run TTA/JMA-ACTION-HISTORY.postman_collection.json -e 'Truck_Trailer.postman_environment.json' --reporters cli,htmlextra --reporter-htmlextra-export newman/report.html",
    "api-jma-trailers-release": "newman run TTA/trailer.postman_collection.json -e 'Truck_Trailer.postman_environment.json' -r html",
    "api-jma-trucks-release": "newman run TTA/trucks.postman_collection.json -e 'Truck_Trailer.postman_environment.json' -r html"
  },
  "author": "MamurjonUzokov",
  "dependencies": {
    "newman": "^5.3.0"
  }
}
